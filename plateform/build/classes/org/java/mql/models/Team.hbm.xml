<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.java.mql.models.Team" table="TEAM">

		<id name="id" type="long" column="TEAM_ID">
			<generator class="native" />
		</id>

		<property name="name" column="NAME" type="string" />


		<list name="etudiants" cascade="all">
			<key column="ETUDIANT_ID" />
			<list-index column="ETUDIANTS_INDEX" />
			<one-to-many class="org.java.mql.models.Etudiant" />
		</list>
		
		<list name="projects" cascade="all">
			<key column="PROJECT_ID" />
			<list-index column="PROJECTS_INDEX" />
			<one-to-many class="org.java.mql.models.Project" />
		</list>
		
		<one-to-one name="teamLeader" class="org.java.mql.models.TeamLeader" />
	</class>
</hibernate-mapping>
