<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.java.mql.models.p2.Project" table="PROJECT">

		<meta attribute="class-description">
			This class contains the project detail.
		</meta>
		<!-- private long id; -->
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>

		<!-- -->
		<property name="name" column="NAME" type="string" />

		<!-- -->
		<property name="desc" column="DESC" type="string" />

		<!-- -->
		<property name="datePublication" column="DATE_PUBLICATION"
			type="string" />

		<!-- -->
		<property name="deadLine" column="DEAD_LINE" type="string" />
		<many-to-one name="matiere" class="org.java.mql.models.p2.Matiere" />
		<many-to-one name="liverable" class="org.java.mql.models.p2.Liverable" />
		<many-to-one name="team" class="org.java.mql.models.p1.Team" />

	</class>
</hibernate-mapping>
