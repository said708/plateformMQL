<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.java.mql.models.File" table="FILE"
		discriminator-value="A">

		<meta attribute="class-description">
			This class contains the file detail.
		</meta>

		<!-- private long id; -->
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>

		<!-- private String name; -->
		<property name="name" column="NAME" type="string" />

		<!-- private String path; -->
		<property name="path" column="PATH" type="string" />
		
		<!-- private String size; -->
		<property name="size" column="SIZE" type="string" />
		
		<!-- private String uploadDate; -->
		<property name="uploadDate" column="UPLOAD_DATE" type="string" />

		<!-- private Liverable liverable; -->
		<many-to-one name="liverable" class="org.java.mql.models.Liverable"
			column="LIVERABLE" />

		<!-- <subclass name="org.java.mql.models.File" discriminator-value="F" 
			/> -->
	</class>
</hibernate-mapping>
