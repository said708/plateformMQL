<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "Team" table = "TEAM">
      
      <meta attribute = "class-description">
         This class contains the team detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "name" column = "NAME" type = "string"/>
      
      <set name = "etudiants" cascade="all">
         <key column = "ID_STUDENT"/>
         <one-to-many class="org.java.mql.models.p1.Etudiant" />
      </set>
      <set name = "projects" cascade="all">
         <key column = "ID_PROJECT"/>
         <one-to-many  class="org.java.mql.models.p2.Project" />
      </set>
      <many-to-one name="matiere" class="org.java.mql.models.p2.Matiere" />
      
   </class>
</hibernate-mapping>
